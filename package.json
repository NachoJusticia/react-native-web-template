{
  "name": "react-native-web-template",
  "version": "0.0.1",
  "author": {
    "name": "Ignacio Justicia Ramos",
    "email": "ignaciojusticia@gmail.com"
  },
  "scripts": {
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "clear-cache": "gulp clear-cache",
    "eslint": "./node_modules/.bin/eslint .",
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "start": "react-native start",
    "web-dev-bundle": "webpack-dev-server --content-base web/public/ --config web/webpack/web.dev.config.js --port 3001 --inline --hot --colors",
    "web-bundle": "webpack --config web/webpack/web.prod.config.js --progress --colors"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-native": "^0.42.0",
    "react-redux": "^3.1.2",
    "redux": "^3.1.1",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react-native": "^3.2.1",
    "gulp": "^3.9.0",
    "less": "^2.5.3",
    "less-loader": "^4.1.0",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.4.0",
    "style-loader": "^0.23.1",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
